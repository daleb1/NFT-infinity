<% title = "Create Item" %>
<div class="bg-gray-900 pb-4 sm:pt-32">
  <div class="mx-auto max-w-7xl px-6 lg:px-8 animate animate-fade-up">
    <div class="min-w-0 flex-wrap md:px-8 lg:px-0 xl:col-span-6 mt-5 items-center justify-center">
      <div class="mx-auto max-w-2xl lg:text-center">
        <p class="mt-2 text-3xl font-bold tracking-tight text-white sm:text-4xl">Connect Your Wallet</p>
        <p class="mt-6 text-lg leading-2 text-gray-300"> Lorem Ipsum is simply dummy text of the printing and typesetting
          industry.</p>
      </div>
    </div>
  </div>
</div>
<section>
  <div class="bg-white">
    <div class="bg-gray-900 pt-24 sm:pt-32 pb-4">
    </div>
    <div class="mx-auto max-w-2xl px-4 py-16 sm:px-6 sm:py-24 lg:max-w-7xl lg:px-8">
      <div class="lg:grid lg:grid-cols-2 lg:items-start lg:gap-x-8">
        <!-- Preview Item -->
        <div
        class="flex flex-col pt-0 px-0 pb-4 items-center justify-start gap-[16px]">
        <p class="font-bold text-2xl">Preview Item</p>
        <%- include('../../components/atom/live-auction-2'); %>
              <%- include('../../components/atom/live-auction-1',
              { imgSrc: "../../assets/images/profile-thumbnail/dale.png"}); %>
        </div>
        
        <!-- Product info -->
        <div class="px-4 py-6 sm:p-8">
          <div class="grid max-w-2xl grid-cols-1 gap-x-6 gap-y-8 sm:grid-cols-6">
            <!--Upload File-->
